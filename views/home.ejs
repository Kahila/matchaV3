            </svg>
        </b> </a>
        <a href="javascript:void(0);" style="font-size:15px;" class="icon" onclick="myFunction()">&#9776;</a>
    </div>

    <div class="users">
        <% for (var item=(users.length -1); item >= 0; item--){%>
            <div class="container">
                <div class="pic">
                    <div class="rating-box">
                        <% if (fame[item] == 0){%>
                            <span class="rating-star empty-star"></span>
                            <span class="rating-star empty-star"></span>
                            <span class="rating-star empty-star"></span>
                            <span class="rating-star empty-star"></span>
                            <span class="rating-star empty-star"></span>
                            <% } %>
                                <% if (fame[item] == 1){%>
                                    <span class="rating-star full-star"></span>
                                    <span class="rating-star full-star"></span>
                                    <span class="rating-star full-star"></span>
                                    <span class="rating-star full-star"></span>
                                    <span class="rating-star full-star"></span>
                                    <% } %>
                                        <% if (fame[item] == 2){%>
                                            <span class="rating-star full-star"></span>
                                            <span class="rating-star full-star"></span>
                                            <span class="rating-star empty-star"></span>
                                            <span class="rating-star empty-star"></span>
                                            <span class="rating-star empty-star"></span>
                                            <% } %>
                                                <% if (fame[item] == 3){%>
                                                    <span class="rating-star full-star"></span>
                                                    <span class="rating-star full-star"></span>
                                                    <span class="rating-star full-star"></span>
                                                    <span class="rating-star empty-star"></span>
                                                    <span class="rating-star empty-star"></span>
                                                    <% } %>
                                                        <% if (fame[item] == 4){%>
                                                            <span class="rating-star full-star"></span>
                                                            <span class="rating-star full-star"></span>
                                                            <span class="rating-star full-star"></span>
                                                            <span class="rating-star full-star"></span>
                                                            <span class="rating-star empty-star"></span>
                                                            <% } %>
                                                                <% if (fame[item] == 5){%>
                                                                    <span class="rating-star full-star"></span>
                                                                    <span class="rating-star full-star"></span>
                                                                    <span class="rating-star full-star"></span>
                                                                    <span class="rating-star full-star"></span>
                                                                    <span class="rating-star full-star"></span>
                                                                    <% } %>
                    </div>
                    <!-- <div class="arrow-box"><svg class="arrow" width="24" height="24" viewBox="0 0 24 24">
						<path d="M11,4H13V16L18.5,10.5L19.92,11.92L12,19.84L4.08,11.92L5.5,10.5L11,16V4Z"></path>
					  </svg></div> -->
                    <img class="kratos" src="<%= img[item]%>">
                </div>
                <div class="name-box">
                    <h2>
                        <%= users[item] %>, <span class="age"> <%= age[item]%> </span></h2>
                    <p>bio</p>
                </div>
                <div class="footer">
                    <div class="circle" id="circletwo">
                        <a href="">
                            <svg class="star" width="24" height="24" viewBox="0 0 24 24">
							<path d="M6 6h-6v-6h6v6zm9-6h-6v6h6v-6zm9 0h-6v6h6v-6zm-18 9h-6v6h6v-6zm9 0h-6v6h6v-6zm9 0h-6v6h6v-6zm-18 9h-6v6h6v-6zm9 0h-6v6h6v-6zm9 0h-6v6h6v-6z">
							</path>
						</svg>
                        </a>
                    </div>
                    <div class="circle" id="circlethree">
                        <a href="http://localhost:3000/request/request?id=<%= id[item]%>">
                            <svg class="heart" width="24" height="24" viewBox="0 0 24 24">
							<path d="M15.653 19.415c-1.162 1.141-2.389 2.331-3.653 3.585-6.43-6.381-12-11.147-12-15.808 0-4.005 3.098-6.192 6.281-6.192 2.197 0 4.434 1.042 5.719 3.248 1.279-2.195 3.521-3.238 5.726-3.238 3.177 0 6.274 2.171 6.274 6.182 0 1.269-.424 2.546-1.154 3.861l-1.483-1.484c.403-.836.637-1.631.637-2.377 0-2.873-2.216-4.182-4.274-4.182-3.257 0-4.976 3.475-5.726 5.021-.747-1.54-2.484-5.03-5.72-5.031-2.315-.001-4.28 1.516-4.28 4.192 0 3.442 4.742 7.85 10 13l2.239-2.191 1.414 1.414zm7.347-5.415h-3v-3h-2v3h-3v2h3v3h2v-3h3v-2z">
							</path>
						</svg>
                        </a>
                    </div>
                </div>
            </div>
            <%} %>
    </div>
    <script>
        function myFunction() {
            var x = document.getElementById("myTopnav");
            if (x.className === "topnav") {
                x.className += " responsive";
            } else {
                x.className = "topnav";
            }
        }
    </script>
</body>

</html>